{
    "name": "vue-image-touch",
    "version": "1.0.8",
    "description": "touch image of a modal on mobile",
    "author": "Bruce yan",
    "homepage": "",
    "license": "MIT",
    "main": "lib/index.js",
    "keywords": [
        "vue",
        "image",
        "mobile",
        "swipe",
        "scale",
        "drag"
    ],
    "scripts": {
        "start": "NODE_ENV=dev webpack-dev-server --mode development example/index.ts --open",
        "build": "webpack --mode production src/index.ts",
        "test": "jest"
    },
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "lib/*",
        "LICENSE",
        "types.ts",
        "README.md",
        "package.json"
    ],
    "dependencies": {
        "vue-touch": "^2.0.0-beta.4"
    },
    "devDependencies": {
        "@types/jest": "^24.0.18",
        "@vue/component-compiler-utils": "^3.0.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "git-tslint": "^1.0.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-vue-preprocessor": "^1.7.0",
        "less": "^3.10.3",
        "less-loader": "^5.0.0",
        "rimraf": "^3.0.0",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.2.0",
        "typescript": "^3.6.3",
        "vue": "^2.6.10",
        "vue-hot-reload-api": "^2.3.4",
        "vue-loader": "^15.7.1",
        "vue-property-decorator": "^8.3.0",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.2"
    },
    "lint-staged": {
        "*.{js,json,ts,css,md,vue,react}": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -e $GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
